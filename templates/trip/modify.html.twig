{% extends 'base.html.twig' %}

{% block title %} Modification de la sortie "{{ form.vars.value.name }}" {% endblock %}

{% block body %}

    <div class="container">

        <div>
            <h1>Modifier de la sortie "{{ form.vars.value.name }}"</h1>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <input type="submit" class="btn btn-primary" name="Enregistrer" value="Enregistrer">
            {{ form_end(form) }}
        </div>

        <div>
            <input type="submit" class="btn btn-success"name="Publier" value="Publier la sortie">
        </div>
        <div>
            <input type="submit" class="btn btn-danger"name="Supprimer" value="Supprimer la sortie">
        </div>
        <div>
            <a href="{{ path('welcome') }}"><button class="btn btn-warning">Annuler</button></a>
        </div>

    </div>

{% endblock %}



